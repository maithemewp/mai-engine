{
    "name": "control-color-palette",
    "version": "1.0.3",
    "description": "Development toolkit for Kirki's control-color-palette",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kirki-framework/control-color-palette.git"
    },
    "keywords": [
        "kirki-color-palette"
    ],
    "author": "Kirki Framework",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kirki-framework/control-color-palette/issues"
    },
    "homepage": "https://github.com/kirki-framework/control-color-palette#readme",
    "dependencies": {
        "react": "17.*",
        "react-dom": "17.*"
    },
    "devDependencies": {
        "@babel/preset-react": "7.*",
        "@parcel/transformer-sass": "^2.0.0-rc.0",
        "parcel": "^2.0.0-rc.0"
    },
    "alias": {
        "jquery": {
            "global": "jQuery"
        },
        "react": {
            "global": "React"
        },
        "react-dom": {
            "global": "ReactDOM"
        }
    },
    "scripts": {
        "build": "parcel build  src/control.js --dist-dir dist",
        "dev": "parcel build  src/control.js --no-optimize --dist-dir dist",
        "watch": "parcel watch src/control.js --dist-dir dist"
    }
}
